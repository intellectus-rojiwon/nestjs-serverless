{
    "name": "nestjs-serverless",
    "version": "0.0.0",
    "description": "nestjs + serverless template",
    "main": "build/main",
    "scripts": {
        "check": "ts-patch check",
        "postinstall": "husky",
        "prepare": "ts-patch install && typia patch",
        "prettier:fix": "prettier . -w",
        "----------------------------------------------": "",
        "sdk": "rimraf ./sdk && NODE_ENV=test nestia sdk",
        "build": "rimraf ./build && tsc -p ./tsconfig.build.json",
        "build:prisma": "rimraf ./prisma/client && prisma generate",
        "build:swagger": "rimraf ./packages/api/swagger/swagger.json && NODE_ENV=test nestia swagger",
        "build:sdk": "npm run sdk && rimraf ./packages/sdk/lib && tsc -p ./tsconfig.sdk.json && rimraf ./sdk",
        "build:nestia": "npm run build:sdk && npm run build:swagger",
        "build:test": "rimraf ./tbuild && tsc -p ./tsconfig.test.json",
        "-----------------------------------------------": "",
        "start": "npm run build && NODE_ENV=development node build/main",
        "test": "npm run build:test && NODE_ENV=test node tbuild/test",
        "swagger": "npm run build:swagger && node ./packages/api/swagger/index.cjs",
        "-------------------------------------------------": "",
        "db:sync": "npm run build:prisma && prisma migrate dev -n _",
        "db:deploy": "prisma migrate deploy",
        "sls:deploy": "sls deploy"
    },
    "author": "intellectus-rojiwon",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "https://github.com/intellectus-rojiwon/nestjs-serverless.git"
    },
    "keywords": [
        "nestjs",
        "nestia",
        "serverless"
    ],
    "dependencies": {
        "@aws-sdk/client-cloudwatch-logs": "^3.525.0",
        "@codegenie/serverless-express": "^4.13.0",
        "@nestia/core": "^2.5.12",
        "@nestia/fetcher": "^2.5.12",
        "@nestjs/common": "^10.3.3",
        "@nestjs/core": "^10.3.3",
        "@nestjs/platform-express": "^10.3.3",
        "@nestjs/terminus": "^10.2.3",
        "aws-lambda": "^1.0.7",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "helmet": "^7.1.0",
        "rxjs": "^7.8.1",
        "typia": "^5.4.13",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@nestia/e2e": "^0.4.1",
        "@nestia/sdk": "^2.5.12",
        "@prisma/client": "^5.10.2",
        "@project/api": "file:packages/api",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/aws-lambda": "^8.10.134",
        "@types/cookie-parser": "^1.4.6",
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.22",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "eslint-plugin-deprecation": "^2.0.0",
        "husky": "^9.0.11",
        "nestia": "^5.2.2",
        "prettier": "^3.2.5",
        "prisma": "^5.10.2",
        "prisma-markdown": "^1.0.9",
        "rimraf": "^5.0.5",
        "serverless": "^3.38.0",
        "serverless-jetpack": "^0.11.2",
        "serverless-offline": "^13.3.3",
        "serverless-plugin-typescript": "^2.1.5",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "ts-patch": "^3.1.2",
        "typescript": "^5.3.3",
        "typescript-transform-paths": "^3.4.7"
    }
}
